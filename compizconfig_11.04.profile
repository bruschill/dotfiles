[gnomecompat]
s0_run_command_terminal_key = <Control><Alt>t

[opengl]
s0_texture_filter = 2

[ring]
s0_next_key = <Alt>Tab

[wall]
s0_preview_timeout = 0.050000
s0_border_width = 5
s0_slide_duration = 0.050000

[obs]
s0_opacity_matches = Tooltip | Menu | PopupMenu | DropdownMenu;
s0_opacity_values = 90;

[shift]
s0_initiate_key = <Alt>Tab
s0_shift_speed = 0.500000
s0_mode = 1
s0_reflection = false

[grid]
s0_outline_color = #3030309f
s0_fill_color = #3030304f

[put]
s0_put_left_key = Disabled
s0_put_right_key = Disabled
s0_put_top_key = Disabled
s0_put_bottom_key = Disabled
s0_put_topleft_key = Disabled
s0_put_topright_key = Disabled
s0_put_bottomleft_key = Disabled
s0_put_bottomright_key = Disabled

[place]
s0_mode_matches = class=Gnome-terminal;
s0_mode_modes = 1;
s0_viewport_matches = name=chromium-browser;class=VirtualBox;name=gvim;class=Sqliteman;name=empathy;class=Evolution;name=skype;name=Banshee;name=ubuntu-tweak;name=software-center;class=Transmission-gtk;
s0_viewport_x_values = 1;1;1;1;2;2;2;2;2;2;2;
s0_viewport_y_values = 1;2;2;2;1;1;1;1;2;2;2;

[fade]
s0_fullscreen_visual_bell = true

[animation]
s0_minimize_effects = animation:Sidekick;
s0_minimize_durations = 175;

[unityshell]
s0_launcher_hide_mode = 1
s0_icon_size = 32

[core]
s0_active_plugins = core;bailer;detection;composite;opengl;compiztoolbox;decor;switcher;vpswitch;commands;resize;gnomecompat;snap;regex;move;obs;imgpng;grid;place;mousepoll;animation;workarounds;unitymtgrabhandles;wall;session;fade;scale;unityshell;
s0_autoraise = false
s0_autoraise_delay = 500
s0_maximize_window_key = Disabled
s0_show_desktop_key = <Super>d
s0_toggle_window_maximized_key = <Control>m
s0_toggle_window_shaded_key = Disabled

[switcher]
s0_speed = 0.750000
s0_timestep = 0.600000
s0_focus_on_switch = true
s0_zoom = 0.000000
s0_minimized = false

[resize]
s0_border_color = #3030309f
s0_fill_color = #30303019

