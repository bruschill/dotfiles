# list
alias ll='ls -alF'
alias la='ls -A'
alias lh='ls -lah'
alias l='ls -CF'

# update
alias uu='sudo apt-get update && sudo apt-get upgrade -y'

# ruby
alias kr='pkill -9 ruby'

# git
g=git
gstat='git status'
gb='git branch'
gba='git branch -a'
gc='git commit -v'
gca='git commit -v -a'
gco='git checkout'
gp='git pull'
gpr='git pull --rebase'
gcount='git shortlog -sn'
gcp='git cherry-pick'
