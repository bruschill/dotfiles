ZSH=$HOME/.oh-my-zsh
ZSH_HISTORY_PATH=$HOME/.zsh_history
ZSH_CUSTOM=$HOME/.zsh_profile.d
ZSH_THEME="miloshadzic"

DISABLE_AUTO_UPDATE=true

plugins=(vi-mode colored-man)

alias rspec="noglob rspec"

source $ZSH/oh-my-zsh.sh
source $HOME/.aliasrc

eval "$(rbenv init -)"

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
