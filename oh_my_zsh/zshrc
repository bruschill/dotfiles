ZSH=$HOME/.oh-my-zsh
ZSH_HISTORY_PATH=$HOME/.zsh_history
ZSH_CUSTOM=$HOME/.zsh_profile.d

DISABLE_AUTO_UPDATE=true

unset MAILCHECK
unsetopt auto_name_dirs

setopt nullglob

alias npm="noglob npm"

ZSH_THEME="miloshadzic"

plugins=(osx vi-mode npm)

source $ZSH/oh-my-zsh.sh
source $HOME/.aliasrc

export PATH=/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/share/npm/bin:/.rvm/bin
