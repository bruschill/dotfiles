[gnomecompat]
s0_run_command_terminal_key = <Control><Alt>t

[wall]
s0_preview_timeout = 0.050000
s0_preview_scale = 200
s0_border_width = 5
s0_slide_duration = 0.050000
s0_mmmode = 1

[obs]
s0_opacity_matches = Tooltip | Menu | PopupMenu | DropdownMenu;
s0_opacity_values = 90;

[put]
s0_put_left_key = Disabled
s0_put_right_key = Disabled
s0_put_top_key = Disabled
s0_put_bottom_key = Disabled
s0_put_topleft_key = Disabled
s0_put_topright_key = Disabled
s0_put_bottomleft_key = Disabled
s0_put_bottomright_key = Disabled

[rotate]
s0_acceleration = 4.400000
s0_speed = 20.000000

[place]
s0_mode_matches = class=Gnome-terminal;class=Gvim;
s0_mode_modes = 1;3;
s0_viewport_matches = class=Chromium-browser;class=Empathy;class=Skype;class=banshee;class=Gvim;class=VirtualBox;class=Ubuntu-tweak;class=Software-center;
s0_viewport_x_values = 1;2;2;2;1;1;2;2;
s0_viewport_y_values = 1;1;1;1;2;2;2;2;

[fade]
s0_fullscreen_visual_bell = true

[core]
s0_active_plugins = core;bailer;detection;composite;opengl;compiztoolbox;decor;vpswitch;resize;gnomecompat;snap;regex;move;imgpng;grid;place;mousepoll;animation;workarounds;wall;session;ezoom;staticswitcher;fade;scale;
s0_outputs = 1920x1080+0+0;
s0_autoraise = false
s0_autoraise_delay = 500
s0_maximize_window_key = Disabled
s0_show_desktop_key = <Super>d
s0_toggle_window_maximized_key = <Alt>F10
s0_toggle_window_shaded_key = Disabled

[resize]
s0_border_color = #3030309f
s0_fill_color = #30303019

