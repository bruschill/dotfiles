[decoration]
as_shadow_radius = 9.000000

[core]
as_active_plugins = core;ccp;move;resize;place;decoration;mousepoll;png;svg;gnomecompat;commands;regex;session;imgjpeg;dbus;text;vpswitch;minimize;workarounds;obs;fade;cube;snap;resizeinfo;rotate;staticswitcher;scale;scaleaddon;
